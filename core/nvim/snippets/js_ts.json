{
  "console.log": {
    "prefix": [
      "con",
      "cl"
    ],
    "body": [
      "console.log($1);"
    ],
    "description": "Log output to console"
  },
  "Export": {
    "prefix": [
      "export"
    ],
    "body": [
      "export function ${TM_FILENAME_BASE}() {",
      "  $0",
      "}"
    ],
    "description": "Creates a export function with the function name set to the current file's base name"
  },
  "Use Effect": {
    "prefix": [
      "useEffect"
    ],
    "body": [
      "useEffect(() => {",
      "  $1",
      "}, [$2]);"
    ]
  },
  "For Loop": {
    "prefix": [
      "for"
    ],
    "body": [
      "for (let i = 0; $1 ;++i) {",
      "}"
    ]
  }
}
